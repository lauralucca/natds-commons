<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="Theme.{{brandName}}.{{mode}}" parent="Theme.MaterialComponents.{{materialMode}}">
    {{#each color}}
    <item name="{{name}}">{{value}}</item>
    {{/each}}
    
    {{#each size}}
    <item name="{{name}}">{{value}}dp</item>
    {{/each}}
    
    {{#each spacing}}
    <item name="{{name}}">{{value}}dp</item>
    {{/each}}
    
    {{#each assets}}
    {{#each @this}}
    <item name="{{name}}">{{value}}</item>
    {{/each}}
    {{/each}}
  </style>
</resources>